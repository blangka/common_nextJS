

{
  "presets": [
    "@babel/preset-typescript",
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@emotion",
      {
        "sourceMap": true,
        "autoLabel": "dev-only",
        "labelFormat": "[dirname]-[local]",
        "cssPropOptimization": true
      }
    ]
  ]
}